static void run_U_TRIG(void *p)
{	

  fb_t *pb = (fb_t *)p;
  vam_t clk = pb->ins[1].v;
  vam_t &out = pb->outs[1].v;
  vam_t &old_clk = pb->props[0].v;

 
	if(!old_clk->v().b() && clk->v().b()){
		out->mutable_v()->set_b(true);
	}else{
		out->mutable_v()->set_b(false);
	}
	old_clk->mutable_v()->set_b(clk->v().b());
}


static fb_t fb_U_TRIG = {{"Trigger","U_TRIG", "",0,0,0,0,run_U_TRIG},
                        {{"EN",PIN_NO_LOCK, T_BOOL, "", nullptr},
                         {"CLK", PIN_NO_LOCK,T_BOOL, "", nullptr}},
                        {{"STATUS", PIN_NO_LOCK,T_INT32, "", nullptr},
                         {"Q", PIN_NO_LOCK,T_BOOL, "", nullptr}},
						 {{"OLD_CLK", PIN_NO_LOCK,T_BOOL, "", nullptr}}};