static void run_U_TRIG(void *p)
{	
	fb_t *pb = (fb_t *)p;

	if(!pb->d[2].v.v.b && pb->d[0].v.v.b){
		pb->d[1].v.v.b = 1;
	}else{
		pb->d[1].v.v.b = 0;
	}
	pb->d[2].v.v.b = pb->d[0].v.v.b;
}

static fb_t fb_U_TRIG = {
	"U_TRIG", 1, 1, 1, 0, run_U_TRIG, 
	{		
		"CLK", T_BOOL, 0,
		"Q", T_BOOL, 0,
		"OLD_CLK", T_BOOL, 0,
	}
};
