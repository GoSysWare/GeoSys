static void run_D_DT_VAL(void *p)
{	
	fb_t *pb = (fb_t *)p;

	Time dt = clk_cycle();

	Bool AM = pb->d[0].v.v.b;
	Bool KO = pb->d[1].v.v.b;
	Bool AO = pb->d[2].v.v.b;
	Bool PO = pb->d[3].v.v.b;
	Bool OPN = pb->d[4].v.v.b;
	Bool FO = pb->d[5].v.v.b;
	Bool KO_FB = pb->d[6].v.v.b;
	Bool LO = pb->d[7].v.v.b;
	Bool MPP = pb->d[8].v.v.b;
	Bool LC = pb->d[9].v.v.b;
	Bool KC_FB = pb->d[10].v.v.b;
	Bool FZ = pb->d[11].v.v.b;
	Bool CLS = pb->d[12].v.v.b;
	Bool PC = pb->d[13].v.v.b;
	Bool AC = pb->d[14].v.v.b;
	Bool KC = pb->d[15].v.v.b;
	Bool DIS = pb->d[16].v.v.b;
	Bool L_R = pb->d[17].v.v.b;
	Bool TS = pb->d[18].v.v.b;
	Time TP = pb->d[19].v.v.tm;
	Time TD = pb->d[20].v.v.tm;


	pb->d[33].v.v.b = (((AM && AO && !AC) || (KO && !KO_FB && !KC)|| PO) && FO && !DIS && !L_R && !LO && !PC) && !OPN;
	pb->d[34].v.v.b = (((AM && AC && !AO) || (KC && !KC_FB && !KO)|| PC) && FZ && !DIS && !L_R && !LC) && !CLS;
	pb->d[21].v.v.b = (((AM && AO && !AC) || (KO && !KO_FB && !KC)|| PO)&& !pb->d[33].v.v.b);
	pb->d[23].v.v.b = (((AM && AC && !AO) || (KC && !KC_FB && !KO)|| PC)&& !pb->d[34].v.v.b);
	pb->d[28].v.v.b = FO;
	pb->d[29].v.v.b = FZ;
	
	pb->d[36].v.v.b = (OPN || MPP || KC);
	pb->d[39].v.v.b = (CLS || MPP || KO);
	pb->d[35].v.v.b = (pb->d[33].v.v.b && !pb->d[38].v.v.b);
	pb->d[38].v.v.b = (pb->d[34].v.v.b && !pb->d[35].v.v.b);
			

	if(pb->d[35].v.v.b){
		pb->d[37].v.v.b = 1;
	}else if(pb->d[36].v.v.b){
		pb->d[37].v.v.b = 0;
	}
	pb->d[30].v.v.b = PO;
	
	
	if(pb->d[38].v.v.b){
		pb->d[40].v.v.b = 1;
	}else if(pb->d[39].v.v.b){
		pb->d[40].v.v.b = 0;
	}
	if(!TS){
		pb->d[22].v.v.b = pb->d[37].v.v.b && !pb->d[26].v.v.b;
		pb->d[24].v.v.b = pb->d[40].v.v.b && !pb->d[26].v.v.b;
	}else{
		pb->d[22].v.v.b = pb->d[37].v.v.b && pb->d[50].v.v.b;
		pb->d[24].v.v.b = pb->d[40].v.v.b && pb->d[50].v.v.b;
	}
	pb->d[31].v.v.b = PC;
	

	pb->d[25].v.v.b = MPP;


	pb->d[41].v.v.b = MPP;
	pb->d[42].v.v.b = pb->d[35].v.v.b || pb->d[38].v.v.b;
	if(pb->d[41].v.v.b){
		pb->d[43].v.v.b = 1;
	}else if(pb->d[42].v.v.b){
		pb->d[43].v.v.b = 0;
	}
	
	pb->d[44].v.v.b = pb->d[37].v.v.b || pb->d[40].v.v.b;
	pb->d[47].v.v.b = OPN && CLS && !pb->d[43].v.v.b;


	if(pb->d[44].v.v.b){
		if(!pb->d[46].v.v.b){
			pb->d[32].v.v.tm = 0;
			pb->d[52].v.v.tm = 0;
		}else if(pb->d[52].v.v.tm >= TD){
			pb->d[45].v.v.b = 1;
			pb->d[32].v.v.tm = TD;
			pb->d[52].v.v.tm = TD;
		}else{
			pb->d[45].v.v.b = 0;
			pb->d[52].v.v.tm += dt;
			pb->d[32].v.v.tm = pb->d[52].v.v.tm; 
		}
	
	
		if(!pb->d[51].v.v.b){
			pb->d[53].v.v.tm = 0;
		}else if(pb->d[53].v.v.tm >= TP){
			pb->d[50].v.v.b = 0;
			pb->d[53].v.v.tm = TP;
		}else{
			pb->d[50].v.v.b = 1;
			pb->d[53].v.v.tm += dt;
		}
	}else{
		pb->d[45].v.v.b = 0;
		pb->d[50].v.v.b = 1;
		pb->d[32].v.v.tm = 0;
	}
	pb->d[46].v.v.b = pb->d[44].v.v.b;
	pb->d[51].v.v.b = pb->d[44].v.v.b;


	if(pb->d[47].v.v.b){
		if(!pb->d[49].v.v.b){
			pb->d[54].v.v.tm = 0;
		}else if(pb->d[54].v.v.tm >= 20){
			pb->d[48].v.v.b = 1;
			pb->d[54].v.v.tm = 20;
		}else{
			pb->d[48].v.v.b = 0;
			pb->d[54].v.v.tm += dt;
		}
	}else{
		pb->d[48].v.v.b = 0;
	}
	pb->d[49].v.v.b = pb->d[47].v.v.b;
	pb->d[26].v.v.b = pb->d[45].v.v.b || pb->d[48].v.v.b;


	pb->d[27].v.v.i = (Int)OPN + (Int)CLS * 2 + (Int)(!(pb->d[22].v.v.b + pb->d[24].v.v.b) && 
			!(OPN + CLS) && !pb->d[26].v.v.b && !pb->d[25].v.v.b) * 3 + (Int)(DIS || pb->d[26].v.v.b) * 4;
}

static fb_t fb_D_DT_VAL = {
	"D_DT_VAL", 21, 12, 22, 0, run_D_DT_VAL, 
	{
		"AM", T_BOOL, 0,
		"KO", T_BOOL, 0,
		"AO", T_BOOL, 0,
		"PO", T_BOOL, 0,

		"OPN", T_BOOL, 0,
		"FO", T_BOOL, 0,
		"KO_FB", T_BOOL, 0,
		"LO", T_BOOL, 0,

		"MPP", T_BOOL, 0,
		"LC", T_BOOL, 0,
		"KC_FB", T_BOOL, 0,
		"FZ", T_BOOL, 0,

		"CLS", T_BOOL, 0,
		"PC", T_BOOL, 0,
		"AC", T_BOOL, 0,
		"KC", T_BOOL, 0,

		"DIS", T_BOOL, 0,
		"L_R", T_BOOL, 0,
		"TS", T_BOOL, 0,
		"TP", T_TIME, 0,
		"TD", T_TIME, 0,

		"ONENO", T_BOOL, 0,
		"ONCMD", T_BOOL, 0,
		"OFFENO", T_BOOL, 0,
		"OFFCMD", T_BOOL, 0,

		"MPPT", T_BOOL, 0,
		"TIMOUT", T_BOOL, 0,
		"STU", T_INT, 0,

		"V_FO", T_BOOL, 0,
		"V_FZ", T_BOOL, 0,
		"V_PO", T_BOOL, 0,
		"V_PC", T_BOOL, 0,

		"ETD", T_TIME, 0,


		"OP", T_BOOL, 0,
		"CL", T_BOOL, 0,
		"OpenS", T_BOOL, 0,
		"OpenR", T_BOOL, 0,
		"OpenQ", T_BOOL, 0,

		"CloseS", T_BOOL, 0,
		"CloseR", T_BOOL, 0,
		"CloseQ", T_BOOL, 0,

		"MPPS", T_BOOL, 0,
		"MPPR", T_BOOL, 0,
		"MPPQ", T_BOOL, 0,

		"TDS", T_BOOL, 0,
		"TDQ", T_BOOL, 0,
		"old_TD", T_BOOL, 0,

		"MPTDS", T_BOOL, 0,
		"MPTDQ", T_BOOL, 0,
		"old_MPT", T_BOOL, 0,

		"TPQ", T_BOOL, 0,
		"old_TP", T_BOOL, 0,

		"td", T_TIME, 0,
		"tp", T_TIME, 0,
		"tmpp", T_TIME, 0,
	}
};
