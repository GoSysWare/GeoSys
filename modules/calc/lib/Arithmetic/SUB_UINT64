static void run_SUB_UINT64(void *p) {

  fb_t *pb = (fb_t *)p;
  vam_t &enable = pb->ins[0].v;
  vam_t &status = pb->outs[0].v;
  if (enable->v().b() == false)
    return;
  vam_t &out = pb->outs[1].v;
  vam_t in1 = pb->ins[1].v;
  vam_t in2 = pb->ins[2].v;
  if (in1->v().ull() < in2->v().ull()) {
    status->mutable_v()->set_i(-2);
  } else {
    out->mutable_v()->set_ull(in1->v().ull() - in2->v().ull());
  }
}

static fb_t fb_SUB_UINT64 = {
    {"Arithmetic", "SUB_UINT64", "", 0, 0, 0, 0, run_SUB_UINT64},
    {{"EN", PIN_NO_LOCK, T_BOOL, "", nullptr},
     {"IN1", PIN_NO_LOCK, T_UINT64, "", nullptr},
     {"IN2", PIN_NO_LOCK, T_UINT64, "", nullptr}},
    {{"STATUS", PIN_NO_LOCK, T_INT32, "", nullptr},
     {"OUT", PIN_NO_LOCK, T_UINT64, "", nullptr}}};
