static void run_XOR(void *p)
{	
	fb_t *pb = (fb_t *)p;

	if(pb->d[0].v.b && pb->d[1].v.b){
		pb->d[2].v.b = 0;
		return;
	}
	if(!pb->d[0].v.b && !pb->d[1].v.b){
		pb->d[2].v.b = 0;
		return;
	}

	pb->d[2].v.b = 1; 
}


/*
static void run_XOR(fb_t *pb)
{
	pb->d[2].v.b = pb->d[0].v.b ^ pb->d[1].v.b; 
}
*/


static fb_t fb_XOR = {
	"XOR", 2, 1, 0, 0, run_XOR, 
	{
		"IN1", T_BOOL, 0,
		"IN2", T_BOOL, 0,
		"OUT", T_BOOL, 0
	}
};
