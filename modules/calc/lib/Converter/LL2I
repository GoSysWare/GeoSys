
static void run_LL2I(void *p) {
  fb_t *pb = (fb_t *)p;
  vam_t &enable = pb->ins[0].v;
  vam_t &status = pb->outs[0].v;
  if (enable->v().b() == false)
    return;
  // 强制转化溢出时会警告，但还是会执行
  if(pb->ins[1].v->v().ll() > INT_MAX || pb->ins[1].v->v().ll() < INT_MIN ){
    status->mutable_v()->set_i(1);
  }
  pb->outs[1].v->mutable_v()->set_i((int)pb->ins[1].v->v().ll());
}
static fb_t fb_LL2I = {{"Converter", "LL2I", "", 0, 0, 0, 0, run_LL2I},
                      {{"EN", PIN_NO_LINK, T_BOOL, "", nullptr},
                       {"IN", PIN_NO_LINK, T_INT64, "", nullptr}},
                      {{"STATUS", PIN_NO_LINK, T_INT32, "", nullptr},
                       {"OUT", PIN_NO_LINK, T_INT32, "", nullptr}}};

