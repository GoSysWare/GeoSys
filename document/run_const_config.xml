<?xml version="1.0" encoding="utf-8"?>
<!--这是GeoSys运行态所有的配置信息-->
<!--在系统内部一些变量都是以int型传递-->
<!--在应用程序，如界面接收到int数字-->
<!--则需要根据此文件解析成相应的字符串-->
<GeoSys version="1.0.1">
	<!-- 单位 -->
	<UNIT size="256">  
		<unitenum id="1"> Kg </unitenum>
		<unitenum id="2"> Km </unitenum>
		<unitenum id="3">C</unitenum>
		<unitenum id="4" >P</unitenum>
		<unitenum id="5" >m</unitenum>
		<unitenum id="6" >A</unitenum>
		<unitenum id="7" >A</unitenum>
	</UNIT>
		<!-- 报警级别 -->
	<ALARMCLASS size="256" >  
		<alarmclassenum id="1" > Red </alarmclassenum>
		<alarmclassenum id="2" > Yellow </alarmclassenum>
		<alarmclassenum id="3" > Blue </alarmclassenum>
		<alarmclassenum id="4" > Width </alarmclassenum>
		<alarmclassenum id="5" > Black </alarmclassenum>
		<alarmclassenum id="6" > Green </alarmclassenum>
		<alarmclassenum id="7" > Orange </alarmclassenum>
	</ALARMCLASS>
</GeoSys>


